{
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
    "rules": {
        "indent": [
            2,
            2,
            {
                "VariableDeclarator": {
                    "var": 2,
                    "let": 2,
                    "const": 3
                }
            }
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "comma-style": [
          2,
          "last"
        ],
        "curly": 2,
        "eqeqeq": 2
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended"
}
